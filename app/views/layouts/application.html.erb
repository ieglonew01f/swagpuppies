<!DOCTYPE html>
<html>
  <head>
    <title><%= t('site_title') %> - <%= yield :title %></title>
    <%= csrf_meta_tags %>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application'%>
    <%= yield :pagecss %>
  </head>
  <body>
	<%= render "header/nav_bar" %>
	<%= yield %>
	<%= yield :content %>
	<%= render "footer/footer" %>
	<%= javascript_include_tag 'application'%>
  </body>
</html>
